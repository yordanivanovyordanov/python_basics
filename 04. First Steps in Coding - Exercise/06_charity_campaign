days = int(input())
bakers = int(input())

cakes = int(input())
waffles = int(input())
crepes = int(input())

cakes_price = 45
waffles_price = 5.8
crepes_price = 3.2

daily_sum = (cakes * cakes_price + waffles * waffles_price + crepes * crepes_price) * bakers
total_sum = daily_sum * days

charity_money = 7/8 * total_sum
print(charity_money)
