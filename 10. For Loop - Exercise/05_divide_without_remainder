n = int(input())

p1 = 0
p2 = 0
p3 = 0
sum_p1 = 0
sum_p2 = 0
sum_p3 = 0

for numbers in range(0, n):
    num = int(input())
    if num % 2 == 0:
        p1 += 1
    if num % 3 == 0:
        p2 += 1
    if num % 4 == 0:
        p3 += 1

print('{0:.2f}'.format(p1 / n * 100) + '%')
print('{0:.2f}'.format(p2 / n * 100) + '%')
print('{0:.2f}'.format(p3 / n * 100) + '%')
