word = input()

a = 1
e = 2
i = 3
o = 4
u = 5

total_sum = 0

for letter in word:

    if letter == 'a':
        total_sum += a

    elif letter == 'e':
        total_sum += e

    elif letter == 'i':
        total_sum += i

    elif letter == 'o':
        total_sum += o

    elif letter == 'u':
        total_sum += u

print(total_sum)
